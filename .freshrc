fresh freshshell/fresh bin/fresh --bin
# A full-featured & carefully designed adaptive prompt for Bash & Zsh
# <https://github.com/nojhan/liquidprompt/blob/master/liquidprompt>
fresh nojhan/liquidprompt liquidprompt

# Literal files
fresh .vimrc --file
fresh .tmux.conf --file
fresh .zshrc --file
fresh .xmodmap --file
fresh .Xresources --file
fresh .xinitrc --file
fresh .liquidpromptrc --file
fresh timecard --bin
fresh dmenu_time --bin
fresh lock.sh --bin
